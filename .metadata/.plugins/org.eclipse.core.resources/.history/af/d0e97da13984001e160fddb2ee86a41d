package com.one;

public class Student {
	
	
	private int rollno;
	private String branch;
	private String university;
	
	
	//default constructor
	public Student() {
		
	}
	
	
	//parameterized constructor
	
	public Student(int rollno, String branch, String university) {
		
		this.rollno = rollno;
		this.branch = branch;
		this.university = university;
	}
	
	// toString
	
	@Override
	public String toString() {
		return "Student [rollno=" + rollno + ", branch=" + branch + ", university=" + university + "]";
	}
	
	
	

}
