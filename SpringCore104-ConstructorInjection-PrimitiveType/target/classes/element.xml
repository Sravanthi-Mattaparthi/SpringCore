<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean name="id1" class ="com.one.Location">
<constructor-arg><value>Kakinada</value></constructor-arg>
<constructor-arg><value>AndhraPradesh</value></constructor-arg>
<constructor-arg><ref bean="id2"></ref></constructor-arg>

</bean>


<bean name="id2" class ="com.one.Student">
<constructor-arg><value>5211</value></constructor-arg>
<constructor-arg><value>PED</value></constructor-arg>
<constructor-arg><value>JNTUK</value></constructor-arg>
</bean>

</beans>